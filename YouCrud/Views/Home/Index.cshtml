@model IEnumerable<YouCrud.Models.EmployeeInfo>
@{
    ViewBag.Title = "Index";
}
<h1>Employee Detail</h1>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Contact)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.About)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Gender)
        </th>
        <th>
            <p>Action</p>
        </th>
    </tr>
    @foreach (var item in Model)
    {

    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Contact)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.About)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Gender)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.EmpoyeeID }) 
            @Html.ActionLink("Details", "Details", new { id = item.EmpoyeeID }) 
            @Html.ActionLink("Delete", "Delete", new { id = item.EmpoyeeID })

        </td>
    </tr>
    }

</table>



